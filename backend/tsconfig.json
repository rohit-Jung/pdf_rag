{
	"compilerOptions": {
		"target": "ES2023",
		"strict": true,
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"module": "NodeNext",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"rootDir": "src",
		"outDir": "out"
	},
	"include": ["src"],
	"exclude": ["node_modules", "out"]
}
